{
  "name": "ts-haskell",
  "version": "1.0.0",
  "description": "Mapping Haskell typeclasses to typescript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint './src/**/*.ts' --max-warnings=0",
    "lint:fix": "eslint './src/**/*.ts' --max-warnings=0  --fix",
    "test": "tap run --node-arg=\"-r\"  --node-arg=\"tsconfig-paths/register\"",
    "coverage:html": "tap report html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxgherman/ts-haskell.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maxgherman/ts-haskell/issues"
  },
  "homepage": "https://github.com/maxgherman/ts-haskell#readme",
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@types/tap": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "8.23.0",
    "@typescript-eslint/parser": "8.23.0",
    "eslint": "9.20.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "globals": "^15.14.0",
    "prettier": "3.4.2",
    "tap": "21.0.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.2"
  }
