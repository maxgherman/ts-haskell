{
  "name": "ts-haskell",
  "version": "1.0.0",
  "description": "Mapping Haskell typeclasses to typescript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint './src/**/*.ts' --max-warnings=0",
    "test": "tap --node-arg=\"-r\"  --node-arg=\"tsconfig-paths/register\" --ts",
    "test:w": "npm test -- --watch --no-check-coverage --no-coverage-report",
    "test:ci": "npm test -- --no-check-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxgherman/ts-haskell.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maxgherman/ts-haskell/issues"
  },
  "homepage": "https://github.com/maxgherman/ts-haskell#readme",
  "devDependencies": {
    "@types/tap": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "tap": "^18.5.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  }
}
